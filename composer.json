    "description": "Adminlte",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Ram Shrestha",
            "email": "sendmail4ram@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2": "*",
      "yiisoft/yii2-bootstrap": "*",
      "cebe/yii2-gravatar": "1.*",
      "bower-asset/admin-lte": "2.*",
      "rmrevin/yii2-fontawesome": "2.9.*"
